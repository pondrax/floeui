{
	"name": "floeui",
	"version": "0.0.8",
	"description": "An adaptation of the [DaisyUI] plugin for [UnoCSS]",
	"keywords": [
		"unocss",
		"daisyui",
		"tailwind",
		"ui framework",
		"svelte"
	],
	"homepage": "https://github.com/pondrax/floeui#readme",
	"author": "Pondra <pondrax3@gmail.com> (https://floeui.vercel.app/)",
	"license": "Apache-2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"start": "vite build && vite preview",
		"package": "svelte-kit sync && svelte-package",
		"package:watch": "svelte-kit sync && svelte-package -w",
		"publish": "svelte-kit sync && svelte-package && npm publish ./package --public",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@iconify-json/bx": "^1.1.5",
		"@sveltejs/adapter-auto": "^1.0.2",
		"@sveltejs/kit": "^1.3.1",
		"@sveltejs/package": "^1.0.2",
		"@types/prismjs": "^1.26.0",
		"dayjs": "^1.11.7",
		"prismjs": "^1.29.0",
		"svelte": "^3.55.1",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^5.0.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.4",
		"unocss": "^0.49.0",
		"vite": "^4.0.4"
	},
	"type": "module"
}